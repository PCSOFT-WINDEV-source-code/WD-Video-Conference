         code : |1-
          // Procedure used to detect a new stream
          // nStreamID is an integer variable containing the identifier of the stream found
          PROCEDURE NewStream(nStreamID)
          
          sCallerName is string
          // Searches for information regarding the calling number
          sCallerName = StreamInfo(nStreamID,StreamCallerName)
          
          gfsIPAddressVideo = StreamInfo(nStreamID,StreamCallerNumber)
          
          
          HReadSeekFirst(Correspondents,Address,sCallerName)
          IF HFound(Correspondents) THEN
          	sCallerName = Correspondents.LastName + " " + Correspondents.FirstName
          END
          
          // check to find out whether the window is locked by an addition or a modification
          IF gfbInComm=False THEN	
          	// Confirm to accept the video-conference
          	
          	IF YesNo(sCallerName +" wants to enter into conference with you,","Do you accept?")=Yes THEN
          		si StreamAccept(nStreamID,ConfReceive) = False ALORS
          			retURN
          		FIN
          		gfbInComm =True
          		BTN_CALL..Caption = <§$0003§>
          		GP_OPTIONS..State = Active
          	ELSE
          		StreamReject(nStreamID)
          	END
          ELSE
          	StreamReject(nStreamID)	
          END
         type : 37
       -
         internal_properties : CAAAAAgAAABTJQpxtU4G5v98H7r8xzi5+w2MfOitUxPHanuYqCVvt7G01mBbeHmNGLOzdJCe5c6f6orA0NTWeAoxlCkkrFr4EdXofzmorJyw8i64Ov7UHk/93McOFzpbHLzJIDtd0zgp2UvFpqcemlx+r/Q=
         code : |1-
          IF gfbCut = False THEN
          	Info("Your correspondent has cut the connection")
          END
          // Update the caption
          BTN_CALL..Caption = <§$0015§>
          gfbInComm = False
          gfbCut = False
          GP_OPTIONS..State = Grayed
         type : 38
  procedures :
   -
     name : Connection
     procedure_id : 1549200149725896371
     type_code : 14
     code : |1-
      // Syntax:
      // Connection (<sAddress>)
      //
      // This procedure is used to connect the video conference stream to the remote user
      // 
      // Parameters:
      //	sAddress: IP address of the correspondent
      
      PROCEDURE Connection(sAddress)
      
      // asks for a connection with the IP address
      ReceiveStream..Identifier = NetworkUser()
      
      // Asks for the connection
      IF StreamConnect(ConfReceive,sAddress) = False THEN
      	Error("The video conference cannot be performed.", "Details: ", ErrorInfo())
      ELSE
      	gfbInComm=True
      	BTN_CALL..Caption = "Finish"
      	GP_OPTIONS..State = Active
      END
     type : 458752
