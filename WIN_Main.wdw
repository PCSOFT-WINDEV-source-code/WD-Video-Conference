      PROCEDURE ListenMessageStartRequest(sChannelName)
      
      sClientInfo is string
      sMessage is string
      
      // Check whether another user is connected
      sClientInfo = SocketClientInfo(sChannelName,SocketAddress)
      IF gfbInDialog = True THEN
      	SocketWrite(sChannelName,csNOTAVAIL)
      	SocketClose(sChannelName)
      	RETURN
      END
      // Analyze the result
      IF NoSpace(sClientInfo)<>"" THEN
      	// Search for the correspondent in the address book
      	HReadSeekFirst(Correspondents,Address,sClientInfo)
      	IF HFound(Correspondents)=False THEN
      		// This correspondent is not recognized
      		Message("Query from an unrecognized user: "+ sClientInfo)
      	ELSE
      		gfsName = Correspondents.LastName +" " +  Correspondents.FirstName
      		
      	END
      		
      	sMessage = SocketRead(NoSpace(sChannelName),False,30000)
      	IF sMessage[[5 TO]] <> "" THEN
      		
      		IF ErrorOccurred THEN
      			// Error while sending the acknowledgement
      			Error("Unable to respond to a request made by a contact",ErrorInfo())		
      			SocketClose(sChannelName)
      		ELSE
      			IF gfsName = ""
      				gfsName = sMessage[[5 TO]]
      			END
      			PostMessage(Handle(WIN_Main),"CONNECTION",1,1)
      				
      			IF EventWait("CONNECTIONSIG", 2000)=True THEN
      				
      				// Wait during 50 hundredths of a second to be sure that the signal is closed if it must be closed
      				CriticalSectionStart()
      					// Save the current address
      					gfsIPAddress = sClientInfo
      				CriticalSectionEnd()
      				
      				// Send the settlement	
      				SocketWrite(NoSpace(sChannelName),csACKOK)
      				
      				// Ask to open the "chat" window	
      				ExecuteMainThread(OpenChat,sChannelName,0)
      				
      				
      			ELSE
      				// Time-out: Connection refused
      				SocketWrite(sChannelName,csREFUSE)
      				SocketClose(sChannelName)
      			END
      		END
      	ELSE
      		// The name is empty so we refuse
      		SocketWrite(NoSpace(sChannelName),csREFUSE)
      	END
      ELSE
      	Error("Unable to define the IP address of the contact",ErrorInfo())	
      END
