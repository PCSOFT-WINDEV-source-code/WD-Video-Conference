     name : OpenChat
     procedure_id : 1549200149726224051
     type_code : 14
     code : |1-
      PROCEDURE OpenChat(LOCAL sSocketName,nCorrespondentID)
      
      // search for the IP address of the correspondent
      sIP is string
      HReadSeekFirst(Correspondents,ParticipantsID,nCorrespondentID)
      IF HFound(Correspondents)=True THEN
      	sIP= Correspondents.LastName + " " + Correspondents.FirstName
      ELSE
      	sIP = ""
      END
      // Open the window used to communicate with the participant
      OpenSister(WIN_Dialog,nCorrespondentID,sSocketName,sIP)
