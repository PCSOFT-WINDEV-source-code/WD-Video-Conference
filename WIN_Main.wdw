     name : SendMessageStartRequest
     procedure_id : 1549200149726158515
     type_code : 14
     code : |1+
      PROCEDURE SendMessageStartRequest(sIP)
      
      // Create a socket name for this connection
      sSocketName is string = Today()+Now()
      
      // Attempt to connect
      Message("Connection to the IP address: "+sIP)
      IF SocketConnect(sSocketName,cnRequestSocketPort,sIP) = False
      
      	// Connection error	
      	Error("Unable to join the correspondent",ErrorInfo());Message()
      	gfbInDialog = False
      	BTN_DIALOG..State = Active
      	RESULT False
      ELSE
      	SocketWrite(sSocketName,"100 " + NetMachineName())
      	// Connection established, waiting for ACOK
      	Message("Connection established, waiting for participant response")
      	SWITCH SocketRead(sSocketName,False,30000)
      		
      		CASE csACKOK :
      			// Connection accepted 
      			Message("Looking for the person called")
      			gfsName=sIP
      			EventDestroy("SIG_DISCUSSION")
      			EventCreate("SIG_DISCUSSION",eventAutomatic,eventClose)
      			
      			// Search for the correspondent in the address book
      			HReadSeekFirst(Correspondents,Address,sIP) 
      			IF HFound(Correspondents)=True THEN
      				// Ask to open the "chat" window for the specified correspondent			
      				Message()
      				ExecuteMainThread(OpenChat,sSocketName,Correspondents.ParticipantsID)	
      				
      			ELSE
      				Message()
      				// Ask to open the "chat" window for the specified correspondent			
      				ExecuteMainThread(OpenChat,sSocketName,0)	
      				
      			END		
      			
      			gfbInDialog = True					
      			RESULT True
      		CASE csNOTAVAIL :
      			// correspondent not available
      			Info("The person called is not available now")
      			Message()
      			gfbInDialog = False
      			BTN_DIALOG..State = Active
      			RESULT False
      			
      		CASE csREFUSE
      			// the correspondent has refused		
      			Info("The correspondent refused to have an immediate discussion with you")
      			Message()
      			gfbInDialog = False
      			BTN_DIALOG..State = Active
      			RESULT False
      		OTHER CASE :
      			// No response from the application of the correspondent
      			Info("The participant did not respond within the timeout, the connection will be abandoned in: "+sIP,ErrorInfo())	
      			Message()
      			gfbInDialog = False
      			BTN_DIALOG..State = Active
      			RESULT False		
      	END	
      END
