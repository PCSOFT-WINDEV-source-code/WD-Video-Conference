     code : |1-
      // Summary: Update the combo box of correspondents
      PROCEDURE RefreshCorrespondentList()
      
      ListDeleteAll(CMB_CALLEDPARTY)
      
      
       FOR EACH Correspondents 
      	ListAdd(CMB_CALLEDPARTY,Correspondents.LastName + [" "] + Correspondents.FirstName + gLink(Correspondents.Address))
      END
      
      IF CMB_CALLEDPARTY..Occurrence = 0 THEN
      	CMB_CALLEDPARTY..State = Grayed
      	BTN_MODIFCONTACT..State = Grayed
      	BTN_DELETE..State = Grayed
      ELSE
      	CMB_CALLEDPARTY..State = Active	
      	BTN_MODIFCONTACT..State = Active
      	BTN_DELETE..State = Active
      END
