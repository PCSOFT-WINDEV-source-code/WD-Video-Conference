      // Syntax:
      // ModifyFlux ()
      //
      
      PROCEDURE ModifyFlux()
      nMode is int
      
      // Update the nMode variable
      // Transmission mode of the stream
      IF CBOX_SENDING[1]= True THEN
      	nMode = StreamOutgoingVideo
      END
      IF CBOX_SENDING[2]=True THEN
      	nMode += StreamOutgoingSound
      END
      IF CBOX_INCOMINGDATA[1]=True THEN
      	nMode += StreamIncomingVideo
      END
      IF CBOX_INCOMINGDATA[2]=True THEN
      	nMode +=StreamIncomingSound
      END
      // Stops the stream
      StreamStop(ReceiveStream)
      // Replay it with the new parameters
      StreamPlay(ReceiveStream,nMode)
