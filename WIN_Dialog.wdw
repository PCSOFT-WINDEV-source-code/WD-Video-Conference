            sSmiley is string
            sSmiley = OpenPopupPosition(WIN_Smiley,poAccordingToControl+poTop+poRight,MySelf)
            
            IF sSmiley <> "" THEN
            	// search for the selected smiley
            	HReadSeekFirst(Smiley,SmileyID,sSmiley)
